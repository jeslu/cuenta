<p id="notice"><%= notice %></p>

<h1>Tipo Equipos</h1>

<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th>Activo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_equipos.each do |tipo_equipo| %>
      <tr>
        <td><%= tipo_equipo.tipo %></td>
        <td><%= tipo_equipo.activo %></td>
        <td><%= link_to 'Show', tipo_equipo %></td>
        <td><%= link_to 'Edit', edit_tipo_equipo_path(tipo_equipo) %></td>
        <td><%= link_to 'Destroy', tipo_equipo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Equipo', new_tipo_equipo_path %>
