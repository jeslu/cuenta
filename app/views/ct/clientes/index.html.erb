<p id="notice"><%= notice %></p>

<h1>Ct Clientes</h1>
<%= render 'form', ct_cliente: @ct_cliente %>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Telefono</th>
      <th>Direccion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ct_clientes.each do |ct_cliente| %>
      <tr>
        <td><%= ct_cliente.nombre %></td>
        <td><%= ct_cliente.telefono %></td>
        <td><%= ct_cliente.direccion %></td>
        <td><%= link_to 'Show', ct_cliente %></td>
        <td><%= link_to 'Edit', edit_ct_cliente_path(ct_cliente) %></td>
        <td><%= link_to 'Destroy', ct_cliente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ct Cliente', new_ct_cliente_path %>
