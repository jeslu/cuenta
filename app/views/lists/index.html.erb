<p id="notice"><%= notice %></p>

<h1>Lists</h1>

<div class="container  ">

<div class="columns ">
 
    <% @lists.each do |list| %>
     <div class="column ">
      <%= list.name %>
      <%= link_to 'Show', list %>
      <%= link_to 'Edit', edit_list_path(list) %>
      <%= link_to 'Destroy', list, method: :delete, data: { confirm: 'Are you sure?' } %>
    </div>
    <% end %>
    </div> 
 </div>

<br>

<%= link_to 'New List', new_list_path %>
