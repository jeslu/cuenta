<!DOCTYPE html>
<html>
  <head>
    <title>J&G App Web</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag "#{@URL_DOMAIN}/libs/bulma/bulma.min.css?=#{@VRS_ASSETS}",media: 'all' , 'data-turbolinks-track': 'reload'%>
  </head>

  <body>

  <div>
  <% if signed_in? %>
  <%= render "_shared/menu"%>
  <% else %>
    <%= link_to "Registrarse", new_user_registration_path %>
    <%= link_to "Ingresar", new_user_session_path %>
  <% end %>
</div>

  <div class="container">
    <%= yield %>
</div>


  </body>

</html>
