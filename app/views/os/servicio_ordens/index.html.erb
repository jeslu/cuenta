<h1>Os Servicio Ordens</h1>

<table>
  <thead>
    <tr>
      <th>Orden servicio</th>
      <th>Servicio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @os_servicio_ordens.each do |os_servicio_orden| %>
      <tr>
        <td><%= os_servicio_orden.orden_servicio_id %></td>
        <td><%= os_servicio_orden.servicio_id %></td>
        <td><%= link_to 'Show', os_servicio_orden %></td>
        <td><%= link_to 'Edit', edit_os_servicio_orden_path(os_servicio_orden) %></td>
        <td><%= link_to 'Destroy', os_servicio_orden, method: :delete, data: { confirm: 'Are you sure?' } %></td>
