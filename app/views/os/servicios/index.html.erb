
<div class="container border  ">
<p id="notice"><%= notice %></p>
<div class=" border-bottom border-info">
  <h1>Servicios</h1>

</div>
<br>


<!--<h1>Total <%= @os_servicios %> </h1>-->

<div class="table-responsive-sm">
  <table class="table table-borderless">
    <thead>
      <tr class="table-info">
        <th>Servicio</th>
        <th>Costo</th>
        <th>Descripcion</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @os_servicios.each do |os_servicio| %>
        <tr>
          <td><%= link_to os_servicio.servicio, os_servicio%></td>
          <td><%= os_servicio.costo %></td>
          <td><%= os_servicio.descripcion %></td>
          <!--<td><%= link_to 'Show', os_servicio %></td>-->
          <td><%= link_to 'Edit', edit_os_servicio_path(os_servicio) %></td>
          <td><%= link_to 'Destroy', os_servicio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

</div>
</div>

<br>

<%= link_to 'New Os Servicio', new_os_servicio_path %>
