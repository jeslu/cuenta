<p id="notice"><%= notice %></p>

<h1>Os Equipos</h1>

<table>
  <thead>
    <tr>
      <th>N serie</th>
      <th>Marca</th>
      <th>Modelo</th>
      <th>Tipo equipo</th>
      <th>Orden servicio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @os_equipos.each do |os_equipo| %>
      <tr>
        <td><%= os_equipo.n_serie %></td>
        <td><%= os_equipo.Marca.marca %></td>
        <td><%= os_equipo.Modelo.modelo %></td>
        <td><%= os_equipo.TipoEquipo.tipo %></td>
        <td><%= os_equipo.orden_servicio_id %></td>
        <td><%= link_to 'Show', os_equipo %></td>
        <td><%= link_to 'Edit', edit_os_equipo_path(os_equipo) %></td>
        <td><%= link_to 'Destroy', os_equipo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Os Equipo', new_os_equipo_path %>
