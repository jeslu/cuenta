<p id="notice"><%= notice %></p>

<h1>Os Orden Servicios</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Cliente</th>
      <th>Servicio</th>
      <th>Equipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @os_orden_servicios.each do |os_orden_servicio| %>
      <tr>
        <td><%= os_orden_servicio.User.nombre %></td>
        <td><%= os_orden_servicio.Cliente.nombre %></td>
        <td><%= os_orden_servicio.Servicio.servicio %></td>
        <td><%= os_orden_servicio.Equipo.n_serie %></td>
        <td><%= link_to 'Show', os_orden_servicio %></td>
        <td><%= link_to 'Edit', edit_os_orden_servicio_path(os_orden_servicio) %></td>
        <td><%= link_to 'Destroy', os_orden_servicio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Os Orden Servicio', new_os_orden_servicio_path %>
